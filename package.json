{
  "name": "@straw-hat/goat",
  "version": "0.0.1",
  "description": "Another Functional Programming Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/goat.git"
  },
  "license": "MIT",
  "keywords": [
    "straw-hat-team"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "prepublishOnly": "yarn build",
    "docs:reference": "typedoc",
    "format": "prettier --write */** --ignore-unknown --no-error-on-unmatched-pattern",
    "ci:install": "yarn install --frozen-lockfile",
    "ci:test:jest": "yarn test --ci --coverage",
    "ci:prettier": "prettier --check */** --ignore-unknown --no-error-on-unmatched-pattern"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@straw-hat/prettier-config": "^2.0.1",
    "@straw-hat/tsconfig": "^2.3.1",
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  }
}
